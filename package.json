    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:reset": "dotenv -e .env.local -- prisma db push --force-reset",
    "db:seed": "tsx prisma/seed/activities.ts",
    "prisma:studio": "prisma studio",
    "sync-users": "dotenv -e .env.local -- tsx scripts/sync-clerk-users.ts",
    "sync-users:prod": "dotenv -e .env.production -- tsx scripts/sync-clerk-users.ts",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate"
